// ==UserScript==
// @name         NJIT Covid Pledge Auto-click
// @namespace    https://github.com/dmjab13/njitcovidpledge
// @version      1.0
// @description  stupid covid pledge NO MORE
// @author       dmjab13
// @match        https://webauth.njit.edu/idp/profile/cas/login;*
// @icon         data:image/jpeg;base64,
// @grant        none
// @require      https://greasyfork.org/scripts/12228/code/setMutationHandler.js
// ==/UserScript==

function checkThem(nodes) {
    nodes.forEach(function(n) { n.checked = true });
};
 // maybe the elements are already on the page
checkThem([].slice.call(document.querySelectorAll('input[type="checkbox"]')));

// but anyway set a MutationObserver handler for them
setMutationHandler(document, 'input[type="checkbox"]', checkThem);

 // maybe the elements are already on the page
checkThem([].slice.call(document.querySelectorAll('input[type="submit"]')));

// but anyway set a MutationObserver handler for them
setMutationHandler(document, 'input[type="submit"]', checkThem);

    setTimeout(document.getElementById('submitbtn').click(),2000);
    document.getElementById('accept').click();
